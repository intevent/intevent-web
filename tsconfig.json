{
  "compileOnSave": true,
  "compilerOptions": {
    "importHelpers": true,
    "jsx": "react",
    "lib": ["dom", "es6"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": false,
    "target": "es5",
    "allowJs": true,
    "checkJs": true
  },
  "include": ["./app/**/*"],
  "exclude": ["node_modules"]
}
